"Vimperator configuration file
" Author: Pablo Bernardo @voylinux

silent qmark g http://google.com

" Remove everithing but navigation bar
set gui=none
set gui=navigation

" Set all completion
set wildmode=full

" No error bells
set errorbells=false

" Leader key
let mapleader=','

nnoremap <C-Insert> :tabnew<CR>
nnoremap <Leader>t  :tabnew<CR>
nmap <silent> <C-k> :tabn<CR>
nmap <silent> <C-j> :tabp<CR>
imap <silent> <C-k> <esc><C-n>
imap <silent> <C-j> <esc><C-p>
nmap <silent> <C-t> :tabs<CR>
